<h1>{{ section.settings.text }}</h1>

{% for block in section.blocks %}
    <a href="{{ block.settings.url }}">{{ block.settings.text }}</a>
{% endfor %}

{% schema %}
{
    "name": "Custom section",
    "settings": [
        {
            "type": "text",
            "id": "text",
            "label": "Text"
        }
    ],
    "presets": [
        { 
            "name": "Default custom section",
            "settings":{
                "text": "Default text"
            },
            "blocks":[
                {
                    "type": "social",
                    "settings":{
                        "url": "https://twitter.com",
                        "text": "Twitter"
                    }
                },
                {
                    "type": "social",
                    "settings":{
                        "url": "https://youtube.com",
                        "text": "Youtube"
                    }
                }
            ]
        },
        { 
            "name": "New Yash custom section",
            "settings":{
                "text": "Yash text"
            } 
        }
        ],
    "tag": "header",
    "class": "custom-section",
    "blocks": [
        {
            "name":"Social link",
            "type":"social",
            "settings": [
                {
                    "type": "text",
                    "id": "text",
                    "label": "Text"
                },
                {
                    "type": "url",
                    "id": "url",
                    "label": "URL"
                }
            ]
        }
    ]
}
{% endschema %}